void sendByte(uint8_t data, bool isData) {
  digitalWrite(DC, isData ? HIGH : LOW);
  digitalWrite(CE, LOW);
  
  for (int8_t i = 7; i >= 0; i--) {
    digitalWrite(DIN, (data & (1 << i)) ? HIGH : LOW);
    digitalWrite(CLK, HIGH);
    digitalWrite(CLK, LOW);
  }
  
  digitalWrite(CE, HIGH);
}

void sendCommand(uint8_t cmd) {
  sendByte(cmd, false);
}

void sendData(uint8_t data) {
  sendByte(data, true);
}

void initDisplay() {
  digitalWrite(RST, LOW);
  delay(10);
  digitalWrite(RST, HIGH);
  delay(10);

  sendCommand(0x21);
  sendCommand(0xB8);
  sendCommand(0x04);
  sendCommand(0x14);
  sendCommand(0x20);
  sendCommand(0x0C);

  // Clear display
  for (int i = 0; i < 504; i++) {
    sendData(0x00);
  }
}

void displayImage(const uint8_t *image) {
  sendCommand(0x80 | 0);
  sendCommand(0x40 | 0);
  
  for (uint16_t i = 0; i < 504; i++) {
    sendData(image[i]);
  }
}

void handleButtons() {
  bool buttonPressed = false;
  
  if (digitalRead(BTN_UP) == LOW) {
    manualImageIndex = (manualImageIndex - 1 + totalManualImages) % totalManualImages;
    autoMode = false;
    buttonPressed = true;
    delay(200);
  }

  if (digitalRead(BTN_DOWN) == LOW) {
    manualImageIndex = (manualImageIndex + 1) % totalManualImages;
    autoMode = false;
    buttonPressed = true;
    delay(200);
  }


  if (digitalRead(BTN_SELECT) == LOW) {
    autoMode = false;
    buttonPressed = true;
    delay(200);
  }

  if (digitalRead(BTN_BACK) == LOW) {
    autoMode = false;
    buttonPressed = true;
    delay(200);
  }


  if (buttonPressed) {
    lastButtonPressTime = millis();  // Reset inactivity timer
  }
}

void autoModeCheck() {
  if (!autoMode && millis() - lastButtonPressTime > autoModeTimeout) {
    autoMode = true;
  }
}

