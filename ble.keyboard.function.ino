void sendByte(uint8_t data, bool isData) {
  digitalWrite(DC, isData ? HIGH : LOW);
  digitalWrite(CE, LOW);
  
  for (int8_t i = 7; i >= 0; i--) {
    digitalWrite(DIN, (data & (1 << i)) ? HIGH : LOW);
    digitalWrite(CLK, HIGH);
    digitalWrite(CLK, LOW);
  }
  
  digitalWrite(CE, HIGH);
}

void sendCommand(uint8_t cmd) {
  sendByte(cmd, false);
}

void sendData(uint8_t data) {
  sendByte(data, true);
}

void initDisplay() {
  digitalWrite(RST, LOW);
  delay(10);
  digitalWrite(RST, HIGH);
  delay(10);

  sendCommand(0x21);  // Extended instruction set
  sendCommand(0xB8);  // Set contrast (Vop = 0xB8)
  sendCommand(0x04);  // Temperature coefficient
  sendCommand(0x14);  // Bias system (1:48)
  sendCommand(0x20);  // Basic instruction set
  sendCommand(0x0C);  // Normal display mode
}

void displayImage(const uint8_t *image) {
  sendCommand(0x80 | 0);  // X = 0
  sendCommand(0x40 | 0);  // Y = 0

  for (uint16_t i = 0; i < 504; i++) {
    sendData(image[i]);
  }
}

void displayText(const char *text) {
  sendCommand(0x80 | 0);  // X = 0
  sendCommand(0x40 | 2);  // Y = 2 (centered)
  

}

void handleButtons() {
  if (digitalRead(BTN_UP) == LOW) {
    imageIndex = (imageIndex - 1 + totalImages) % totalImages;
    
    delay(150); // Debounce delay
  }

  if (digitalRead(BTN_DOWN) == LOW) {
    imageIndex = (imageIndex + 1) % totalImages;
    ;
    delay(50);
  }

  
}


